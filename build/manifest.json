{
  "name": "ChatGPT Prompt Wizard",
  "description": "A chrome extension that gives you additional control over chatGPT",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "img/logo-16.png",
    "32": "img/logo-34.png",
    "48": "img/logo-48.png",
    "128": "img/logo-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/logo-48.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.chunk-44ec8036.2fc9afe5.js"
      ],
      "matches": [
        "https://chat.openai.com/chat*"
      ],
      "css": [
        "assets/index.tsx.74bf64cf.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "resources": [
        "img/logo-16.png",
        "img/logo-34.png",
        "img/logo-48.png",
        "img/logo-128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "resources": [
        "assets/chunk-83225f40.js",
        "assets/chunk-744baaec.js",
        "assets/chunk-44ec8036.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "permissions": [
    "storage",
    "webRequest",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}